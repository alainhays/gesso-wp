{% extends "base.twig" %}

{% block content %}
		<h1>Page.Twig</h1>
		<div class="content-wrapper">
			<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
				<section class="article-content">
					<h2 class="article-title">{{post.title}}</h2>
					<div class="article-body">
						{{post.content}}
					</div>
				</section>
			</article>
		</div><!-- /content-wrapper -->

{% endblock %}